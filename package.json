{
  "name": "emoji-mart-fictioneers",
  "version": "5.2.2",
  "description": "Emoji picker for the web",
  "license": "MIT",
  "homepage": "https://missiveapp.com/open/emoji-mart",
  "repository": {
    "type": "git",
    "url": "https://github.com/paulking86/emoji-mart-fictioneers"
  },
  "source": "src/index.js",
  "main": "dist/main.js",
  "module": "dist/module.js",
  "global": "dist/browser.js",
  "browserslist": "defaults",
  "targets": {
    "main": {
      "includeNodeModules": true
    },
    "module": {
      "includeNodeModules": true
    },
    "global": {
      "source": "src/browser.js",
      "includeNodeModules": true
    }
  },
  "keywords": [
    "emoji",
    "emoticons",
    "emoji-picker",
    "web-component",
    "twemoji"
  ],
  "scripts": {
    "build": "parcel build --no-autoinstall",
    "prepublishOnly": "yarn build"
  },
  "devDependencies": {
    "@parcel/transformer-inline-string": "2.7.0",
    "@parcel/transformer-sass": "2.7.0",
    "autoprefixer": "10.4.12",
    "parcel": "2.7.0",
    "preact": "10.11.2"
  },
  "alias": {
    "react": "preact/compat",
    "react-dom/test-utils": "preact/test-utils",
    "react-dom": "preact/compat",
    "react/jsx-runtime": "preact/jsx-runtime"
  },
  "files": [
    "/dist",
    "LICENSE"
  ],
  "publishConfig": {
    "registry":"https://npm.pkg.github.com"
  }
}
